<keymap version="1" name="kwp - windows" parent="$default">
  <action id="FileChooser.NewFolder">
    <keyboard-shortcut first-keystroke="alt insert" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="ctrl n" second-keystroke="a" />
  </action>
  <action id="GotoChangedFile" />
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="ctrl n" second-keystroke="c" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="ctrl n" second-keystroke="f" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="ctrl n" second-keystroke="s" />
  </action>
</keymap>